<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:v="clr-namespace:System;assembly=mscorlib">
    <v:String x:Key="Score">Score</v:String>
    <v:String x:Key="Level">Level</v:String>
    <v:String x:Key="Lives">Lives</v:String>
    <v:String x:Key="NewGame">New game</v:String>
    <v:String x:Key="TopScore">Top score</v:String>
    <v:String x:Key="ClearScore">Clear top score</v:String>
    <v:String x:Key="NoLibrariesError">No plugins available! Please put your plug-in in Plugins subfolder and restart the game.</v:String>
    <v:String x:Key="IncorrectLibraryError">An error occured when the game was trying to apply one plugin (maybe it was not a plugin). Try to left only last stable plugins and restart the game</v:String>
